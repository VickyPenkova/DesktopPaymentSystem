<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entities.CashierEntity" table="Cashier" schema="DesktopPaymentSystem">
        <id name="cashierId">
            <column name="cashierId" sql-type="int(11)"/>
        </id>
        <property name="firstName">
            <column name="firstName" sql-type="varchar(600)" length="600"/>
        </property>
        <property name="lastName">
            <column name="lastName" sql-type="varchar(600)" length="600"/>
        </property>
    </class>
</hibernate-mapping>