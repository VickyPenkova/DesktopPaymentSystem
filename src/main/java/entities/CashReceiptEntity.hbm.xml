<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entities.CashReceiptEntity" table="CashReceipt" schema="DesktopPaymentSystem">
        <id name="cashReceiptId">
            <column name="cashReceiptId" sql-type="int(11)"/>
        </id>
        <property name="productsAmount">
            <column name="productsAmount" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>