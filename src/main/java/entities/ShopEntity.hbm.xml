<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entities.ShopEntity" table="Shop" schema="DesktopPaymentSystem">
        <id name="shopId">
            <column name="shopId" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(1000)" length="1000"/>
        </property>
        <property name="address">
            <column name="address" sql-type="text"/>
        </property>
    </class>
</hibernate-mapping>